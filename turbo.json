{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "@acme/api#dev": {
      "persistent": true,
      "cache": false,
      "outputs": ["dist/**"]
    },
    "@acme/expo#dev": {
      "persistent": true,
      "cache": false
    },
    "@acme/nextjs#dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "CLERK_SECRET_KEY",
    "BLOB_READ_WRITE_TOKEN",
    "OPENAI_API_KEY",
    "AUTH_REDIRECT_PROXY_URL",
    "AUTH_SECRET",
    "PORT",
    "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "API_ORIGIN",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "WEBHOOK_SECRET",
    "NEXT_PUBLIC_AXIOM_TOKEN",
    "NEXT_PUBLIC_AXIOM_DATASET",
    "EXPO_PUBLIC_ENVIRONMENT",
    "EXPO_PUBLIC_API_ORIGIN",
    "API_PASSWORD",
    "UPSTASH_REDIS_URL",
    "UPSTASH_REDIS_TOKEN",
    "EXPO_PUBLIC_REVENUE_APPLE_API_KEY",
    "EXPO_PUBLIC_REVENUE_ANDROID_API_KEY"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}
